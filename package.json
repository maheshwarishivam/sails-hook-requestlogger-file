{
  "name": "sails-hook-requestlogger-file",
  "version": "2.2.0",
  "description": "Sails JS hook to activate morgan request logging for every request and logs to file system",
  "main": "index.js",
  "scripts": {
    "test": "node tests/runner",
    "test-all": "node tests/runner all",
    "autotest": "mocha --watch --reporter spec tests/**/*Test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maheshwarishivam/sails-hook-requestlogger-file.git"
  },
  "author": "Shivam Maheshwari <sayhello@shivammaheshwari.com> (http://www.shivammaheshwari.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maheshwarishivam/sails-hook-requestlogger-file/issues"
  },
  "homepage": "https://github.com/maheshwarishivam/sails-hook-requestlogger-file",
  "sails": {
    "isHook": true
  },
  "keywords": [
    "sails",
    "sails.js",
    "sails-hook",
    "morgan",
    "log-file",
    "access-log"
  ],
  "dependencies": {
    "morgan": "^1.5.0",
    "file-stream-rotator": "^0.4.1"
  },
  "devDependencies": {
    "glob": "^7.1.3",
    "mocha": "^6.0.2",
    "sails": "^1.1.0"
  },
  "contributors": [
    "Markus Padourek",
    "Shivam Maheshwari",
    "Vinod Pandey",
    "Bharat Parmar"
  ],
  "directories": {
    "test": "tests"
  }
